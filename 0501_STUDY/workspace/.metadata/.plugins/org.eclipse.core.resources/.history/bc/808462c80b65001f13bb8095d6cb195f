package employee.controller;

import java.util.ArrayList;

import employee.model.dao.EmployeeDao;
import employee.model.dto.Employee;

//mvc 패턴 : view(사용자화면) -> 요청 -> controller 가 받음 -> model로 전달함
//model이 결과 리턴 -> controller 가 받아서 view로 리턴 ->view가 받아서 출력
public class EmployeeController {
	private EmployeeDao edao = new EmployeeDao();
	
	//dao가 가진 메소드와 맞춰서 메소드 구성함
	public ArrayList<Employee> selectList(){
		return edao.selectAll();
		
	}
}
 